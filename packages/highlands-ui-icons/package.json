{
  "name": "@highlands/highlands-ui-icons",
  "author": "Church of the Highlands",
  "version": "1.0.1",
  "description": "Highlands Svg Icons converted React components.",
  "main": "dist/index.js",
  "keywords": [
    "icons",
    "react",
    "svg",
    "components",
    "highlands"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/highlands/highlands-ui.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/highlands/highlands-ui/issues"
  },
  "homepage": "https://github.com/highlands/highlands-ui/packages/highlands-ui-icons",
  "scripts": {
    "start": "webpack --watch",
    "build": "mkdir -p dist && cp -r src/* dist/",
    "release": "yarn publish",
    "src:icons": "svgr -d src svgs && create-index -r -i ./src"
  },
  "devDependencies": {
    "create-index": "^2.4.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  }
}
